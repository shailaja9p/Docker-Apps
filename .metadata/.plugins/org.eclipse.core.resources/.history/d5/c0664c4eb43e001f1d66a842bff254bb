		System.out.println(empName1==empName2);
		System.out.println(empName1.hashCode()+"   "+empName2.hashCode());
		
	String empName3=empName1;
			System.out.println(empName3.equals(empName1));
	System.out.println(empName3==empName1);
		
		
		
		
		
		
